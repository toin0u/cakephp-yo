    "description" : "Yo plugin for CakePHP 3.x",
    "type"        : "cakephp-plugin",
    "license"     : "MIT",
    "keywords"    : ["cakephp", "yo"],
    "homepage"    : "https://github.com/toin0u/cakephp-yo",

    "authors" : [
        {
            "name"     : "Antoine Corcy",
            "email"    : "contact@sbin.dk",
            "homepage" : "http://sbin.dk",
            "role"     : "Developer"
        }
    ],

    "support" : {
        "issues" : "https://github.com/toin0u/cakephp-yo/issues",
        "source" : "https://github.com/toin0u/cakephp-yo"
    },

    "require" : {
        "php"             : ">=5.4.16",
        "cakephp/cakephp" : "~3.0",
        "toin0u/yo"       : "~1.0"
    },

    "autoload" : {
        "psr-4" : {
